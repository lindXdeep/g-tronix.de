{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": false,
        "**/.dccache": true,
        "README.md": true,
        "target/": true,
    },

    "java.project.exportJar.targetPath": "${workspaceFolder}/index.php",

    "workspaceCacheClean.silent": false,
